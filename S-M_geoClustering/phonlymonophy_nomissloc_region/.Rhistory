library(treeio)
library(ggimage)
library(ggtree)
library(slatkin.maddison)
# Set path. Import tree files & location data.
setwd("C:/Users/Guest1/fgmp_working_dir/projects/HIVcrf01ae_molclock_reruns/6.phonlymonocompleteloc_subsamalt_geoclustSM/subsam_alt_phylo_region")
fast_tree_file <- "ph_only_monoclade.fixnam.fasttre.nexus"
fasttre <- read.nexus(fast_tree_file)
geo_data <- read.delim("ph_only_monoclade.fixnam.loc.tsv")
# Visualize trees.
fasttre_geo <- ggtree(fasttre, right=FALSE) %<+% geo_data
fasttre_geo2 <- fasttre_geo + geom_tippoint(aes(color=region))
fasttre_geo2
# Perform SM test on trees
run_sm(fasttre, geo_data, "region", rep=999)
